<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

<script>

$(editAddressValidation).validate({
    rules:{
        name: {
          required: true,
          minlength: 2,
          pattern:/^[a-zA-Z]+( [a-zA-Z]+)*$/
        },
        phoneNumber: {
          required: true,
          minlength: 10,
          maxlength: 10,
        },
        houseNumber: {
          required: true,
          pattern:"^\\d+/?\\d*[a-zA-Z]?(?<!/)$"
        },
        city: {
          required: true,
          pattern:/^[a-zA-Z0-9]/
        },
        state: {
          required: true,
        },
        pincode:{
            required:true,
            minlength:6,
            maxlength:6
        }
    },
    submitHandler:async function(editAddressValidation,e){
        e.preventDefault();
        let data = {
                id: editAddressValidation.id.value,
                name: editAddressValidation.name.value,
                phoneNumber: editAddressValidation.phoneNumber.value,
                houseNumber: editAddressValidation.houseNumber.value,
                city: editAddressValidation.city.value,
                state: editAddressValidation.state.value,
                pincode: editAddressValidation.pincode.value
            };
        try {
        const response=await fetch("/editAddressData",{
            method:"POST",
            headers:{
                "Content-Type":"application/json"
            },
            body:JSON.stringify(data)
        })
        const value=await response.json();
        if(value.success){
            await Swal.fire({
                        icon: 'success',
                        title: 'Address updated',
                        showConfirmButton: false,
                        timer: 1500
                    });
            window.location.href="/myAddress"
        }else{
            await Swal.fire({
                title: "sorry",
                text: "maximum limit exceeded",
                icon: "error",
                })
        }
        } catch (error) {
            console.error(`error while fetching the data from add address \n ${error}`);
        }
        
    }
})

</script>

